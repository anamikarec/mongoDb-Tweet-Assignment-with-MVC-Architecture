<html lang="en">
   <body>
       <h1>Users</h1>
       <% users.forEach( user => {%>
         <div>
             <h2 style = "text-decoration: underline;">UserId :  <%= user._id %></h2>
             <h2>User Name : <%= user.username %></h2>
             <p>Email : <%= user.email %></p>
             <a href="/users/<%=user._id %>">Details</a>
             <%- include("./components/footer.ejs") %>
         </div>
      <% }) %>

      </body>
</html>